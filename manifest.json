{
  "manifest_version": 3,
  "name": "YouTube Precision Seeker",
  "version": "1.0",
  "description": "Seek 1s with Shift+Arrow and 3s with Alt+Arrow on YouTube",
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
